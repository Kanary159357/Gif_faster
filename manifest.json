{
    "name" : "obj ext",
    "description" : "my ext",
    "version" : "0.1.0",
    "manifest_version" : 2,
    "content_scripts" :[
        {
            "matches": ["https://*/*"],
            "css" : ["myCss.css"],
            "js": ["jquery.js","script.js"],
            "run_at": "document_end"
        }
    ],
    "options_page": "./options.html",
    "browser_action":{
        "default_popup":"index.html"
    },
    "permissions": [
        "tabs",
        "https://*/*"
    ]
}